    "eslint": "^9.11.1",
    "eslint-plugin-svelte": "^2.43.0",
    "globals": "^15.9.0",
    "postcss": "^8.4.47",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tailwindcss": "^3.4.13",
